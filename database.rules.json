{
  "rules": {
    "globalCanvas": {
      "users": {
        ".read": "auth != null",
        ".write": false,
        "$userId": {
          ".read": "auth != null",
          ".write": "auth != null && auth.uid == $userId",
          ".validate": "auth != null"
        }
      }
    },
    ".read": false,
    ".write": false
  }
}
