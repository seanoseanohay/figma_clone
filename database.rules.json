{
  "rules": {
    "canvases": {
      "$canvasId": {
        "presence": {
          ".read": "auth != null",
          ".write": false,
          "$userId": {
            ".read": "auth != null",
            ".write": "auth != null && auth.uid == $userId",
            ".validate": "!newData.exists() || (auth != null && newData.child('userId').val() == auth.uid)"
          }
        },
        "activeObjects": {
          ".read": "auth != null",
          "$objectId": {
            ".read": "auth != null",
            ".write": "auth != null",
            ".validate": "!newData.exists() || newData.hasChildren(['x', 'y', 'isBeingDragged', 'draggedBy', 'lastUpdate'])"
          }
        },
        "agentStatus": {
          ".read": true,
          "$uid": {
            ".read": true,
            ".write": "auth != null && auth.uid === $uid"
          }
        },
        "agentActions": {
          "$actionId": {
            ".read": true,
            ".write": "auth != null"
          }
        }
      }
    },
    "agentTokens": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid"
      }
    },
    ".read": false,
    ".write": false
  }
}
